[flake8]
max-line-length = 88
extend-ignore = 
    E203,
    W503,
    E501,
    W504,
    B008,
    B905
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    .venv,
    .vscode,
    venv,
    migrations
per-file-ignores =
    __init__.py: F401
    tests/*: S101
    src/database/models.py: F401